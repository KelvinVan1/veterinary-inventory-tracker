{
  "name": "veterinary-inventory-tracker",
  "private": true,
  "version": "1.0.0",
  "description": "VetTrack is a simple and easy to use inventory system for managing veterinary inventories",
  "author": {
    "name": "Kelvin Van"
  },
  "main": "./src/client/electron/index.ts",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"vite\"",
    "vite": "vite",
    "server": "nodemon ./src/server/server.ts",
    "electron": "concurrently \"npm run vite\" \"npm run server\" \"electron .\"",
    "electron:solo": "electron .",
    "build": "concurrently \"tsc\" \"vite build\"",
    "preview": "vite preview",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "dependencies": {
    "dotenv": "^16.3.1",
    "electron-squirrel-startup": "^1.0.0",
    "express": "^4.18.2",
    "mongoose": "^7.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.14.2"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.2.1",
    "@electron-forge/maker-deb": "^6.2.1",
    "@electron-forge/maker-rpm": "^6.2.1",
    "@electron-forge/maker-squirrel": "^6.2.1",
    "@electron-forge/maker-zip": "^6.2.1",
    "@electron-forge/plugin-auto-unpack-natives": "^6.2.1",
    "@types/express": "^4.17.17",
    "@types/nodemon": "^1.19.2",
    "@types/react": "^18.2.16",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@vitejs/plugin-react": "^3.1.0",
    "autoprefixer": "^10.4.14",
    "concurrently": "^7.6.0",
    "electron": "^22.3.18",
    "eslint": "^8.45.0",
    "eslint-plugin-react": "^7.33.0",
    "nodemon": "^2.0.22",
    "postcss": "^8.4.27",
    "tailwindcss": "^3.3.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "vite": "^4.4.7"
  }
}
